{
    "name": "zozlak/quick-rdf-io",
    "type": "library",
    "description": "Collection of parser and serializers for zozlak/quick-rdf (compatible with zozlak/rdfInterface)",
    "keywords": [],
    "homepage": "https://github.com/sweetrdf/quickRdfIo",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Żółtak",
            "email": "zozlak@zozlak.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "zozlak/rdf-constants": "^1.1",
        "zozlak/rdf-helpers": "*",
        "zozlak/rdf-interface": "*",
        "zozlak/quick-rdf": "*",
        "pietercolpaert/hardf": "*"
    },
    "suggest": {
        "zozlak/quick-rdf": "*"
    },
    "autoload": {
        "psr-4": {
            "quickRdfIo\\": "src/quickRdfIo"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2",
        "phpstan/phpstan": "*",
        "squizlabs/php_codesniffer": "*",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload-dev": {
        "psr-4": {
            "quickRdfIo\\": "tests"
        }
    }
}
